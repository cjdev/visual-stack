{
  "name": "@cjdev/visual-stack-docs",
  "version": "2.1.3",
  "private": true,
  "devDependencies": {
    "concurrently": "^3.1.0",
    "copyfiles": "^1.2.0",
    "eslint": "^3.14.1",
    "eslint-plugin-react": "^7.4.0",
    "npm-watch": "^0.1.8",
    "prettier": "^1.7.0",
    "react-scripts": "^0.9.5",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "@cjdev/visual-stack": "^2.1.3",
    "@cjdev/visual-stack-redux": "^2.1.3",
    "express": "^4.14.1",
    "material-ui": "^1.0.0-beta.8",
    "material-ui-icons": "^1.0.0-beta.5",
    "mdi-react": "^2.0.46",
    "morgan": "^1.8.0",
    "ramda": "^0.23.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.2",
    "react-responsive": "^1.3.0",
    "react-router": "^3.0.4",
    "redux": "^3.6.0"
  },
  "proxy": "http://localhost:3001/",
  "watch": {
    "samples": "src/**/*.js"
  },
  "scripts": {
    "samples": "copyfiles \"src/containers/{App,Components/Docs,Icons}/**/*.{js,css}\" build/samples",
    "clean": "rimraf build",
    "build": "react-scripts build && npm run samples",
    "lint": "eslint src",
    "format": "prettier {src,server}/**/*.js --write",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "start": "react-scripts start",
    "server": "concurrently npm-watch \"node ./server\"",
    "docs": "concurrently \"npm run start\" \"npm run server\""
  }
}
