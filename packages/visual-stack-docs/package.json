{
  "name": "@cjdev/visual-stack-docs",
  "version": "3.3.0",
  "private": true,
  "devDependencies": {
    "concurrently": "^4.1.0",
    "copyfiles": "^2.1.0",
    "npm-watch": "^0.6.0",
    "prettier": "^1.13.7",
    "ramda": "^0.26.1",
    "react-scripts": "^2.1.3",
    "rimraf": "^2.6.3"
  },
  "dependencies": {
    "@cjdev/visual-stack": "^3.3.0",
    "@cjdev/visual-stack-redux": "^3.3.0",
    "express": "^4.16.3",
    "material-ui": "^1.0.0-beta.46",
    "material-ui-icons": "^1.0.0-beta.36",
    "mdi-react": "^2.1.19",
    "morgan": "^1.9.1",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-redux": "^5.0.7",
    "react-responsive": "^1.3.4",
    "react-router": "^3.2.1",
    "redux": "^3.7.2"
  },
  "proxy": "http://localhost:3001/",
  "watch": {
    "samples": "src/**/*.js"
  },
  "scripts": {
    "samples": "copyfiles \"src/containers/{App,Components/Docs,Icons,GettingStarted}/**/*.{js,css}\" build/samples",
    "clean": "rimraf build",
    "build": "react-scripts build && npm run samples",
    "lint": "eslint src",
    "format": "prettier {src,server}/**/*.js --write",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "start": "react-scripts start",
    "server": "concurrently npm-watch \"node ./server\"",
    "docs": "concurrently \"npm run start\" \"npm run server\"",
    "watch": "concurrently \"npm run start\" \"npm run server\""
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
